{
  "name": "@swissquote/rollup-plugin-eslint",
  "version": "1.19.1",
  "description": "Verify entry point and all imported files with ESLint",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node '*.js'",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=index.js ava"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swissquote/crafty"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "es2015",
    "eslint",
    "lint"
  ],
  "author": "Bogdan Chadkin <trysound@yandex.ru>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.3.0",
    "ansi-colors": "^4.0.0",
    "ava": "4.2.0",
    "c8": "7.11.2",
    "rollup": "2.73.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "4.2.1",
    "eslint": "8.15.0"
  }
}
