{
  "name": "@swissquote/crafty-runner-rspack",
  "version": "1.28.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swissquote/crafty.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "node ../../utils/build.mjs",
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node src '*.js'",
    "test": "node --test",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src node --test"
  },
  "main": "src/index.js",
  "dependencies": {
    "@rsdoctor/rspack-plugin": "1.5.0",
    "@rspack/core": "1.7.2",
    "@swissquote/crafty": "1.28.1",
    "@swissquote/crafty-commons": "1.28.1",
    "@swissquote/crafty-preset-terser": "1.28.1",
    "anypack-plugin-serve": "0.1.0-beta.1"
  },
  "devDependencies": {
    "bufferutil": "4.0.9",
    "c8": "10.1.3",
    "common-ancestor-path": "1.0.1",
    "expect": "30.2.0",
    "glob-to-regexp": "0.4.1",
    "hash-index": "3.0.0",
    "is-glob": "4.0.3",
    "rspack-chain": "1.4.3",
    "service-names-port-numbers": "2.0.0",
    "utf-8-validate": "6.0.6",
    "webpack-merge": "6.0.1"
  },
  "engines": {
    "node": ">=22.12"
  }
}
