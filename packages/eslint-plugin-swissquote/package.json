{
  "name": "@swissquote/eslint-plugin-swissquote",
  "version": "1.22.3",
  "license": "Apache-2.0",
  "repository": "https://github.com/swissquote/crafty.git",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node '*.js' src",
    "build": "node ../../utils/build.js",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src ava"
  },
  "dependencies": {
    "@swissquote/crafty-commons": "1.22.3",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "ava": "5.3.1",
    "c8": "8.0.0",
    "comment-parser": "1.3.1",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-i": "2.27.5-4",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.8",
    "prettier-linter-helpers": "1.0.0",
    "prettier1": "npm:prettier@1.19.1"
  },
  "engines": {
    "node": ">=16"
  }
}
