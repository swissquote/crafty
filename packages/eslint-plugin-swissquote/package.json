{
  "name": "@swissquote/eslint-plugin-swissquote",
  "version": "1.18.1",
  "license": "Apache-2.0",
  "repository": "https://github.com/swissquote/crafty.git",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node '*.js' src",
    "build": "node ../../utils/build.js",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src ava"
  },
  "dependencies": {
    "@swissquote/crafty-commons": "1.18.1",
    "prettier": "1.19.1",
    "typescript": "4.5.5"
  },
  "peerDependency": {
    "eslint": "^6.0.0 | ^7.0.0 | ^8.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "ava": "4.0.1",
    "c8": "7.11.0",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sonarjs": "0.12.0"
  },
  "engines": {
    "node": ">=12"
  }
}
