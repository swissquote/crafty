{
  "name": "@swissquote/eslint-plugin-swissquote",
  "version": "1.20.0",
  "license": "Apache-2.0",
  "repository": "https://github.com/swissquote/crafty.git",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node '*.js' src",
    "build": "node ../../utils/build.js",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src ava"
  },
  "dependencies": {
    "@swissquote/crafty-commons": "1.20.0",
    "prettier": "1.19.1",
    "typescript": "4.9.4"
  },
  "peerDependency": {
    "eslint": "^6.0.0 | ^7.0.0 | ^8.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "ava": "5.1.0",
    "c8": "7.12.0",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "engines": {
    "node": ">=16"
  }
}
