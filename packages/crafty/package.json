{
  "name": "@swissquote/crafty",
  "version": "1.17.2",
  "license": "Apache-2.0",
  "repository": "https://github.com/swissquote/crafty.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "main": "./src/index.js",
  "bin": "./src/bin.js",
  "scripts": {
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node src '*.js'",
    "build": "node ../../utils/build.js",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src ava"
  },
  "dependencies": {
    "@swissquote/crafty-commons": "workspace:*"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.2"
  },
  "devDependencies": {
    "anymatch": "3.1.2",
    "async-done": "1.3.2",
    "ava": "4.0.1",
    "c8": "7.11.0",
    "camelcase-keys": "7.0.1",
    "chokidar": "3.5.3",
    "copy-anything": "2.0.6",
    "enhanced-resolve": "5.8.3",
    "is-negated-glob": "1.0.0",
    "just-debounce": "1.1.0",
    "loud-rejection": "2.2.0",
    "merge-anything": "4.0.2",
    "pretty-hrtime": "1.0.3",
    "undertaker": "1.3.0",
    "yargs-parser": "21.0.0"
  },
  "engines": {
    "node": ">=12"
  }
}
