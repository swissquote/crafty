    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src node --test"
  },
  "main": "src/index.js",
  "dependencies": {
    "@swc/core": "1.12.1",
    "@swissquote/crafty": "1.28.0",
    "@swissquote/crafty-commons": "1.28.0",
