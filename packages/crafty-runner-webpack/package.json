    "common-ancestor-path": "1.0.1",
    "webpack": "5.99.9",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-chain-5": "8.0.2",
    "webpack-plugin-serve": "1.6.0"
  },
  "devDependencies": {
    "bufferutil": "4.0.9",
    "c8": "10.1.3",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "expect": "29.7.0",
    "glob-to-regexp": "0.4.1",
    "hash-index": "3.0.0",
    "inspectpack": "4.7.1",
    "is-glob": "4.0.3",
    "log-symbols": "6.0.0",
    "service-names-port-numbers": "2.0.0",
