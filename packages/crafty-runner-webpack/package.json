{
  "name": "@swissquote/crafty-runner-webpack",
  "version": "1.18.1",
  "license": "Apache-2.0",
  "repository": "https://github.com/swissquote/crafty.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "node ../../utils/build.js",
    "lint": "node ../crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node src '*.js'",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src ava"
  },
  "main": "src/index.js",
  "dependencies": {
    "@swissquote/crafty": "1.18.1",
    "@swissquote/crafty-commons": "1.18.1",
    "@swissquote/crafty-preset-terser": "1.18.1",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-chain": "6.5.1",
    "webpack-dev-server": "4.7.4"
  },
  "devDependencies": {
    "ava": "4.0.1",
    "c8": "7.11.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "glob-to-regexp": "0.4.1",
    "hash-index": "3.0.0",
    "inspectpack": "4.7.1",
    "is-glob": "4.0.3",
    "log-symbols": "5.1.0",
    "service-names-port-numbers": "2.0.0",
    "webpack-merge": "5.8.0"
  },
  "engines": {
    "node": ">=12"
  }
}
