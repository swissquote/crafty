{
  "name": "@swissquote/postcss-color-rgb",
  "version": "1.18.4",
  "description": "PostCSS plugin to transform W3C CSS Color Module Level 4 rgb() new syntax to more compatible CSS (comma-separated rgb() or rgba())",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "color",
    "rgb",
    "rgba"
  ],
  "author": "David Marchena",
  "license": "MIT",
  "repository": "dmarchena/postcss-color-rgb",
  "bugs": {
    "url": "https://github.com/dmarchena/postcss-color-rgb/issues"
  },
  "homepage": "https://github.com/dmarchena/postcss-color-rgb",
  "main": "src/index.js",
  "files": [
    "src",
    "!**/__tests__"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "postcss-value-parser": "4.2.0"
  },
  "devDependencies": {
    "ava": "4.0.1",
    "c8": "7.11.0",
    "postcss": "8.4.7"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "scripts": {
    "lint": "node ../../packages/crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node src",
    "test": "ava",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src ava"
  },
  "private": true
}
