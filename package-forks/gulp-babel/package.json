{
  "name": "@swissquote/gulp-babel",
  "private": true,
  "version": "1.27.0",
  "description": "Use next generation JavaScript, today",
  "license": "MIT",
  "repository": "babel/gulp-babel",
  "author": "Sindre Sorhus <sindresorhus@gmail.com> (sindresorhus.com)",
  "contributors": [
    "Charles Samborski <demurgos@demurgos.net> (demurgos.net)"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "node ../../packages/crafty-preset-eslint/src/commands/jsLint.js --preset recommended --preset node '*.js'",
    "test": "ava 'test.js'",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=index.js ava 'test.js'"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "babel",
    "transpiler",
    "es2015",
    "es2016",
    "es2017",
    "rewriting",
    "transformation",
    "syntax",
    "codegen",
    "desugaring",
    "javascript",
    "compiler"
  ],
  "dependencies": {
    "plugin-error": "2.0.1",
    "replace-ext": "2.0.0",
    "vinyl-sourcemaps-apply": "0.2.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/plugin-transform-arrow-functions": "7.25.9",
    "@babel/plugin-transform-block-scoping": "7.25.9",
    "@babel/plugin-transform-classes": "7.25.9",
    "ava": "6.2.0",
    "c8": "10.1.3",
    "vinyl": "3.0.0",
    "vinyl-fs": "4.0.0",
    "vinyl-sourcemap": "2.0.0"
  }
}
